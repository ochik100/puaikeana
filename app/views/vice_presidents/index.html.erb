  <%= link_to("<< Back to List of Nominees", {:controller => 'nominees', :action => 'index'}, :class => 'back-link') %>

  <div class="vice_presidents index">
    <h2>Vice President Candidates</h2>

    <table class="listing" summary="President list">
      <tr class="header">
        <th>First Name </th>
        <th>Last Name </th>
      </tr>


      <% @vice_presidents.each do |vice_president| %>
      
		      <tr>
		        <td><%= vice_president.first_name %></td>
		        <td><%= vice_president.last_name %></td>
		      </tr>
		      
      <% end %>
    </table>
  </div> 
