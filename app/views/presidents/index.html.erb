  <div class="presidents index">
    <h2>Presidents</h2>

    <table class="listing" summary="President list">
      <tr class="header">
        <th>First Name </th>
        <th>Last Name </th>
      </tr>


      <% @presidents.each do |president| %>
		      <tr>
		        <td><%= president.nominee_id %></td>
		        <td><%= Nominee.joins(:presidents).pluck(:first_name) %></td>
		        <td><%= Nominee.joins(:presidents).pluck(:last_name) %></td>
		      </tr>
      <% end %>
    </table>
  </div> 
