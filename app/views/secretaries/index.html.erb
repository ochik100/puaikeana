  <%= link_to("<< Back to List of Nominees", {:controller => 'nominees', :action => 'index'}, :class => 'back-link') %>

  <div class="secretaries index">
    <h2>Secretary</h2>

    <table class="listing" summary="Secretary list">
      <tr class="header">
        <th>First Name </th>
        <th>Last Name </th>
      </tr>


      <% @secretaries.each do |secretary| %>
      
		      <tr>
		        <td><%= secretary.first_name %></td>
		        <td><%= secretary.last_name %></td>
		      </tr>
		      
      <% end %>
    </table>
  </div> 
